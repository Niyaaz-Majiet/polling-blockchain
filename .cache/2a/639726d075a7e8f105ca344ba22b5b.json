","original":{"line":63,"column":29}},{"generated":{"line":123,"column":0},"source":"components/CreatePoll.js","original":{"line":61,"column":20}},{"generated":{"line":123,"column":18},"source":"components/CreatePoll.js","original":{"line":60,"column":16}},{"generated":{"line":123,"column":19},"source":"components/CreatePoll.js","original":{"line":38,"column":12}},{"generated":{"line":123,"column":20},"source":"components/CreatePoll.js","original":{"line":37,"column":8}},{"generated":{"line":124,"column":0},"source":"components/CreatePoll.js","original":{"line":70,"column":5}},{"name":"Component","generated":{"line":128,"column":2},"source":"components/CreatePoll.js","original":{"line":6,"column":25}},{"name":"CreatePoll","generated":{"line":130,"column":15},"source":"components/CreatePoll.js","original":{"line":73,"column":15}}],"sources":{"components/CreatePoll.js":"import React, { Component } from 'react';\r\nimport { FormGroup, FormControl, Button } from 'react-bootstrap';\r\nimport { Link } from 'react-router-dom';\r\nimport history from '../history';\r\n\r\nclass CreatePoll extends Component {\r\n    state = {\r\n        topic: '',\r\n        about: ''\r\n    }\r\n\r\n    updateInput = (e) => {\r\n       const {value,name} = e.target;\r\n\r\n       this.setState({\r\n           [name]:value\r\n       });\r\n    }\r\n\r\n    addPoll = () => {\r\n        const {topic,about} = this.state;\r\n\r\n        fetch('http://localhost:3000/api/create-report',{\r\n            method: 'POST',\r\n            headers:{\r\n                'Content-Type': 'application/json'\r\n            },\r\n            body:JSON.stringify({\"data\":{topic,about}})\r\n        }).then(res=>res.json())\r\n        .then((json)=>{\r\n            alert(json.message || json.type);\r\n            history.push('/');\r\n        });\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div className=\"create-poll\">\r\n                <h1>Create Voting Poll</h1>\r\n                <div><Link id=\"single-link\" to='/'>Home</Link></div>\r\n                <br />\r\n                <FormGroup>\r\n                    <FormControl\r\n                        input='text'\r\n                        placeholder='Topic'\r\n                        value={this.state.topic}\r\n                        name='topic'\r\n                        onChange={(e) => this.updateInput(e)}\r\n                    />\r\n                </FormGroup>\r\n                <FormGroup>\r\n                    <FormControl\r\n                        input='text'\r\n                        placeholder='about'\r\n                        name='about'\r\n                        value={this.state.about}\r\n                        onChange={(e) => this.updateInput(e)}\r\n                    />\r\n                </FormGroup>\r\n                <div>\r\n                    <Button\r\n                        onClick={this.addPoll}\r\n                        size=\"lg\"\r\n                    >\r\n                        Submit\r\n                    </Button>\r\n                </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default CreatePoll;"},"lineCount":null}},"error":null,"hash":"eaadadd075cca4f5d27dd7479513f546","cacheData":{"env":{}}}